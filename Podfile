platform :ios, '10.0'

inhibit_all_warnings!                           # 全局禁止显示警告

target 'YZFrame' do

pod 'AvoidCrash'                                # 防止 NSArray、NSDictionary 崩溃闪退
#pod 'AFNetworking', '~> 3.0'                    # 网络访问库
pod 'UIAlertController+Blocks'                  # 弹框提示
pod 'CustomIOSAlertView', '~> 0.9.5'            # 自定义弹出对话视图
pod 'LCActionSheet'                             # 底部弹出选择列表组件
pod 'ActionSheetPicker-3.0'                     # 底部弹出选择器
pod 'MBProgressHUD', '~> 1.1.0'                 # 加载等待提示框
pod 'JDStatusBarNotification'                   # 状态栏提醒
pod 'YBPopupMenu', '~> 1.1.2'                   # pop菜单
pod 'MJRefresh'                                 # 下拉刷新、上拉加载
pod 'YYKit'                                     # YYKit一系列组件
pod 'SDWebImage', '~> 4.0'                      # 网络图片加载
pod 'SDCycleScrollView', '~> 1.75'              # 轮播图

end
